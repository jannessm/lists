{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.json"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api calls",
      "urls": ["api/grocery-categories"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 1000,
        "maxAge": "1d"
      }
    },
    {
      "name": "api calls",
      "urls": ["api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1000,
        "maxAge": "10m"
      }
    }
  ]
}
